<form method="post" action="{{ path('app_feedback_delete', {'id': feedback.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ feedback.id) }}">
    <button class="btn btn-danger"><i class="fa fa-trash" aria-hidden="true"></i>
</button>
</form>
